{
	"generator": "retain_lines",
	"rules": [
		{
			"rule": "convert_require",
			"current": {
				"name": "path",
				"module_folder_name": "init",
				"sources": {
					"pkg": "./Packages"
				},
				"use_luau_configuration": true
			},
			"target": {
				"name": "roblox",
				"rojo_sourcemap": "./sourcemap.json",
				"indexing_style": "wait_for_child"
			}
		},
		{
			"rule": "inject_global_value",
			"identifier": "__DEBUG__",
			"env": "__DEBUG__"
		}
	]
}